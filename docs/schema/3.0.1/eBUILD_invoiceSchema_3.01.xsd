              <xsd:attribute name='Codelist' type='xsd:string' use='optional' />
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name='Contact'>
    <xsd:sequence>
      <xsd:element name='Name' type='xsd:string' minOccurs='0' maxOccurs='1' />
      <xsd:element name='Department' type='xsd:string' minOccurs='0' maxOccurs='1' />
      <xsd:element name='InternalAddress' type='xsd:string' minOccurs='0' maxOccurs='1' />
      <xsd:element name='DDI' type='xsd:string' minOccurs='0' maxOccurs='1' />
      <xsd:element name='Switchboard' type='xsd:string' minOccurs='0' maxOccurs='1' />
      <xsd:element name='Fax' type='xsd:string' minOccurs='0' maxOccurs='1' />
      <xsd:element name='Telex' type='xsd:string' minOccurs='0' maxOccurs='1' />
      <xsd:element name='Email' minOccurs='0' maxOccurs='1'>
        <xsd:simpleType>
          <xsd:restriction base='xsd:string'>
            <xsd:pattern value='.*@.*\..*' />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name='Mobile' type='xsd:string' minOccurs='0' maxOccurs='1' />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name='Buyer'>
    <xsd:sequence>
      <xsd:element name='BuyerReferences' minOccurs='0' maxOccurs='1'>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name='SuppliersCodeForBuyer' type='xsd:string' minOccurs='0' maxOccurs='1' />
            <xsd:element name='TaxNumber' type='xsd:string' minOccurs='0' maxOccurs='1' />
            <xsd:element name='GLN' type='xsd:string' minOccurs='0' maxOccurs='1'>
              <xsd:annotation>
                <xsd:documentation>Number conformant to UCC.EAN specification.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name='Party' type='xsd:string' minOccurs='1' maxOccurs='unbounded' />
      <xsd:element name='Address' type='Address' minOccurs='0' maxOccurs='1' />
      <xsd:element name='Contact' type='Contact' minOccurs='0' maxOccurs='unbounded' />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name='ebisDelivery'>
    <xsd:sequence>
      <xsd:element name='DeliverTo' minOccurs='0' maxOccurs='1'>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name='DeliverToReferences' minOccurs='0' maxOccurs='1'>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name='BuyersCodeForDelivery' minOccurs='0' maxOccurs='1'>
                    <xsd:complexType>
                      <xsd:simpleContent>
                        <xsd:extension base='xsd:string'>
                          <xsd:attribute name='Preserve' type='xsd:boolean' use='optional' />
                        </xsd:extension>
                      </xsd:simpleContent>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name='BuyersCodeForLocation' type='xsd:string' minOccurs='0' maxOccurs='1' />
                  <xsd:element name='GLN' type='xsd:string' minOccurs='0' maxOccurs='1'>
                    <xsd:annotation>
                      <xsd:documentation>Number conformant to UCC.EAN specification.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name='Party' type='xsd:string' minOccurs='1' maxOccurs='unbounded' />
            <xsd:element name='Address' type='Address' minOccurs='0' maxOccurs='1' />
            <xsd:element name='Location' type='xsd:string' minOccurs='0' maxOccurs='1'>
              <xsd:annotation>
                <xsd:documentation>Location within address.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name='Contact' type='Contact' minOccurs='0' maxOccurs='unbounded' />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name='Carrier' type='Carrier' minOccurs='0' maxOccurs='1' />
      <xsd:element name='Quantity' type='Quantity' minOccurs='0' maxOccurs='1' />
      <xsd:element name='EarliestAcceptableDate' type='xdr:dateTime' minOccurs='0' maxOccurs='1' />
      <xsd:element name='LatestAcceptableDate' type='xdr:dateTime' minOccurs='0' maxOccurs='1' />
      <xsd:element name='PreferredDate' type='xdr:dateTime' minOccurs='0' maxOccurs='1' />
      <xsd:element name='SpecialInstructions' type='xsd:string' minOccurs='0' maxOccurs='unbounded' />
      <xsd:element name='DeliveryInformation' type='xsd:string' minOccurs='0' maxOccurs='unbounded' />
      <xsd:element name='Narrative' type='xsd:string' minOccurs='0' maxOccurs='unbounded'>
        <xsd:annotation>
          <xsd:documentation>Use for any general narrative comments.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name='Carrier'>
    <xsd:sequence>
      <xsd:element name='CarrierReferences' minOccurs='0' maxOccurs='1'>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name='BuyersCodeForCarrier' type='xsd:string' minOccurs='0' maxOccurs='1' />
            <xsd:element name='GLN' type='xsd:string' minOccurs='0' maxOccurs='1'>
              <xsd:annotation>
                <xsd:documentation>Number conformant to UCC.EAN specification.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name='Party' type='xsd:string' minOccurs='1' maxOccurs='unbounded' />
      <xsd:element name='Address' type='Address' minOccurs='0' maxOccurs='1' />
      <xsd:element name='Contact' type='Contact' minOccurs='0' maxOccurs='unbounded' />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name='Quantity'>
    <xsd:sequence>
      <xsd:element name='Packsize' type='xsd:string' minOccurs='0' maxOccurs='1' />
      <xsd:element name='Amount' type='xdr:float' minOccurs='0' maxOccurs='1' />
    </xsd:sequence>
    <xsd:attribute name='UOMCode' type='xsd:string' use='optional' />
    <xsd:attribute name='UOMDescription' type='xsd:string' use='optional' />
    <xsd:attribute name='UOMCodelist' type='xsd:string' use='optional' />
  </xsd:complexType>
  <xsd:complexType name='InvoiceTo'>
    <xsd:sequence>
      <xsd:element name='InvoiceToReferences' minOccurs='0' maxOccurs='1'>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name='GLN' type='xsd:string' minOccurs='0' maxOccurs='1'>
              <xsd:annotation>
                <xsd:documentation>Number conformant to UCC.EAN specification.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name='SuppliersCodeForInvoiceTo' type='xsd:string' minOccurs='0' maxOccurs='1' />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name='Party' type='xsd:string' minOccurs='1' maxOccurs='unbounded' />
      <xsd:element name='Address' type='Address' minOccurs='0' maxOccurs='1' />
      <xsd:element name='Contact' type='Contact' minOccurs='0' maxOccurs='unbounded' />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name='InvoiceLine'>
    <xsd:sequence>
      <xsd:element name='LineNumber' minOccurs='1' maxOccurs='1'>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base='xdr:int'>
              <xsd:annotation>
                <xsd:documentation>The line number must be included as a reference in documents relating to this transaction.</xsd:documentation>
              </xsd:annotation>
              <xsd:attribute name='Preserve' type='xsd:boolean' use='optional' />
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name='InvoiceLineReferences' type='InvoiceLineReferences' minOccurs='0' maxOccurs='1' />
      <xsd:any minOccurs='0' namespace='##other' processContents='strict'>
        <xsd:annotation>
          <xsd:documentation>This node allows extension of the eBIS XML spec.  Project specific data from other namespaces may be inserted at this point</xsd:documentation>
        </xsd:annotation>
      </xsd:any>
      <xsd:element name='Product' type='Product' minOccurs='0' maxOccurs='unbounded' />
      <xsd:element name='Quantity' type='Quantity' minOccurs='0' maxOccurs='1' />
      <xsd:element name='Price' minOccurs='1' maxOccurs='unbounded'>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name='Units' type='xsd:string' minOccurs='0' maxOccurs='1' />
            <xsd:element name='UnitPrice' type='xdr:float' minOccurs='1' maxOccurs='1' />
            <xsd:element name='SpecialPriceDescription' type='xsd:string' minOccurs='0' maxOccurs='1' />
          </xsd:sequence>
          <xsd:attribute name='UOMCode' type='xsd:string' use='optional' />
          <xsd:attribute name='UOMDescription' type='xsd:string' use='optional' />
          <xsd:attribute name='UOMCodelist' type='xsd:string' use='optional' />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name='PercentDiscount' type='PercentDiscount_Invoice' minOccurs='0' maxOccurs='unbounded' />
      <xsd:element name='AmountDiscount' type='AmountDiscount' minOccurs='0' maxOccurs='unbounded' />
      <xsd:element name='LineTax' minOccurs='0' maxOccurs='1'>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name='MixedRateIndicator' type='xsd:string' minOccurs='0' maxOccurs='1' />
            <xsd:element name='TaxRate' type='TaxRate' minOccurs='1' maxOccurs='1' />
            <xsd:element name='TaxValue' type='xdr:float' minOccurs='0' maxOccurs='1' />
            <xsd:element name='TaxRef' type='TaxRef' minOccurs='0' maxOccurs='1' />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name='LineTotal' type='xdr:float' minOccurs='1' maxOccurs='1' />
      <xsd:element name='Delivery' type='ebisDelivery' minOccurs='0' maxOccurs='unbounded' />
      <xsd:element name='InvoiceLineInformation' type='xsd:string' minOccurs='0' maxOccurs='unbounded' />
      <xsd:element name='ExtendedDescription' type='xsd:string' minOccurs='0' maxOccurs='unbounded' />
      <xsd:element name='Narrative' type='xsd:string' minOccurs='0' maxOccurs='unbounded' />
      <xsd:element name='LotSerial' minOccurs='0' maxOccurs='unbounded'>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name='LotSerialType' minOccurs='1' maxOccurs='1'>
              <xsd:complexType>
                <xsd:simpleContent>
                  <xsd:extension base='xsd:string'>
                    <xsd:attribute name='Code' type='xsd:string' use='required' />
                    <xsd:attribute name='Codelist' type='xsd:string' use='optional' />
                  </xsd:extension>
                </xsd:simpleContent>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name='LotSerialNumber' type='xsd:string' minOccurs='1' maxOccurs='1' />
            <xsd:element name='Quantity' type='Quantity' minOccurs='0' maxOccurs='1' />
            <xsd:element name='LotSerialDates' minOccurs='0' maxOccurs='unbounded'>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name='BestBeforeDate' type='xdr:dateTime' minOccurs='0' maxOccurs='1' />
                  <xsd:element name='DeliveryDate' type='xdr:dateTime' minOccurs='0' maxOccurs='1' />
                  <xsd:element name='DespatchDate' type='xdr:dateTime' minOccurs='0' maxOccurs='1' />
                  <xsd:element name='OnSaleByDate' type='xdr:dateTime' minOccurs='0' maxOccurs='1' />
                  <xsd:element name='SellByDate' type='xdr:dateTime' minOccurs='0' maxOccurs='1' />
                  <xsd:element name='ShelfLifeDate' type='xdr:dateTime' minOccurs='0' maxOccurs='1' />
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name='Narrative' type='xsd:string' minOccurs='0' maxOccurs='unbounded' />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name='Intrastat' type='Intrastat' minOccurs='0' maxOccurs='1' />
    </xsd:sequence>
    <xsd:attribute name='Action' use='optional'>
      <xsd:simpleType>
        <xsd:restriction base='xsd:string'>
          <xsd:enumeration value='Add' />
          <xsd:enumeration value='Change' />
          <xsd:enumeration value='Delete' />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name='TypeCode' type='xsd:string' use='optional' />
    <xsd:attribute name='TypeDescription' type='xsd:string' use='optional' />
    <xsd:attribute name='TypeCodelist' type='xsd:string' use='optional' />
  </xsd:complexType>
  <xsd:complexType name='InvoiceLineReferences'>
    <xsd:sequence>
      <xsd:element name='ContractOrderReference' type='xsd:string' minOccurs='0' maxOccurs='1' />
      <xsd:element name='CostCentre' type='xsd:string' minOccurs='0' maxOccurs='1' />
      <xsd:element name='GeneralLedgerCode' type='xsd:string' minOccurs='0' maxOccurs='1' />
      <xsd:element name='ProjectCode' minOccurs='0' maxOccurs='1'>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base='xsd:string'>
              <xsd:annotation>
                <xsd:documentation>This value should be included as a reference in documents relating to this transaction.</xsd:documentation>
              </xsd:annotation>
              <xsd:attribute name='Preserve' type='xsd:boolean' use='optional' />
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name='ProjectAnalysisCode' minOccurs='0' maxOccurs='1'>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base='xsd:string'>
              <xsd:annotation>
                <xsd:documentation>This value should be included as a reference in documents relating to this transaction.</xsd:documentation>
              </xsd:annotation>
              <xsd:attribute name='Preserve' type='xsd:boolean' use='optional' />
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name='BuyersOrderNumber' minOccurs='0' maxOccurs='1'>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base='xsd:string'>
              <xsd:annotation>
                <xsd:documentation>This field must be included in either the InvoiceReferences or InvoiceLineReferences</xsd:documentation>
              </xsd:annotation>
              <xsd:attribute name='Preserve' type='xsd:boolean' use='optional' />
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name='OrderLineNumber' type='xsd:string' minOccurs='1' maxOccurs='1' />
      <xsd:element name='BuyersOrderLineReference' type='BuyersOrderLineReference' minOccurs='0' maxOccurs='1' />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name='BuyersOrderLineReference'>
    <xsd:simpleContent>
      <xsd:extension base='xsd:string'>
        <xsd:attribute name='Preserve' use='optional'>
          <xsd:simpleType>
            <xsd:restriction base='xsd:string'>
              <xsd:enumeration value='true' />
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name='Product'>
    <xsd:sequence>
      <xsd:element name='SuppliersProductCode' type='xsd:string' minOccurs='0' maxOccurs='1' />
      <xsd:element name='BuyersProductCode' type='xsd:string' minOccurs='0' maxOccurs='1' />
      <xsd:element name='TradedUnitCode' type='xsd:string' minOccurs='0' maxOccurs='1'>
